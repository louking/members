    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "justMyCode": false,
            "env": {
                "FLASK_APP": "run.py",
                "FLASK_ENV": "development"
            },
            "args": [
                "run",
                "--no-debugger"
            ],
            "jinja": true
        },
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "pwa-chrome",
            "url": "http://dev.localhost:5000/",
            "webRoot": "${workspaceFolder}/members"
        },
        {
            "name": "Python: flask membership update fsrc",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "justMyCode": false,
            "env": {
                "FLASK_APP": "app.py",
                "FLASK_ENV": "development"
            },
            "args": [
                "membership",
                "update",
                "fsrc",
                // "--membershipfile",
                // "C:\\Users\\lking\\Documents\\Lou's Software\\operational\\fsrc\\membercache\\membercache-test.csv",
            ],
            "jinja": true
        },
    ]
}